{
  "name": "solc-cli",
  "version": "0.1.3",
  "description": "solc command line interface",
  "preferGlobal": true,
  "bin": {
    "solc": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "babel ./index.js --out-dir dist",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint:js -- --fix",
    "test": "npm run lint",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://insidersbyte@github.com/InsidersByte/solc-cli.git"
  },
  "keywords": [
    "solc",
    "solc-cli"
  ],
  "author": "Jonathon Kelly <jonathon.kelly@insidersbyte.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/InsidersByte/solc-cli/issues"
  },
  "homepage": "https://github.com/InsidersByte/solc-cli#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "cli": "^0.11.2",
    "mkdirp": "^0.5.1",
    "solc": "^0.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1"
  }
}
